output 'BOE NE135A1M-NY1 Unknown' {
    position 0,480
    alias $INTERNAL
}

output 'LG Electronics LG ULTRAGEAR+ 404NTCZD1195' {
    position 1440,0
    alias $HOME_1
}

output 'Acer Technologies Acer XF240H 0x71606395' {
    position 4000,180
    alias $HOME_2
}

profile home_1 {
    output $INTERNAL enable
    output $HOME_1 enable
    output $HOME_2 enable
}
